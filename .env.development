# Development Environment
API_BASE_URL=https://jsonplaceholder.typicode.com
API_TIMEOUT=10000

# Auth Configuration
JWT_SECRET=your-jwt-secret-key
REFRESH_TOKEN_EXPIRY=7d
ACCESS_TOKEN_EXPIRY=15m

# Feature Flags
