# Production Environment
API_BASE_URL=https://your-production-api.com
API_TIMEOUT=15000

# Auth Configuration
# JWT_SECRET should be loaded from secure secret management (AWS Secrets Manager, etc.)
# Set via deployment pipeline: JWT_SECRET=${JWT_SECRET_FROM_VAULT}
JWT_SECRET=
REFRESH_TOKEN_EXPIRY=2592000
ACCESS_TOKEN_EXPIRY=3600

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_BIOMETRIC_AUTH=true

# Debug Settings
DEBUG_API_LOGS=false
DEBUG_REDUX_LOGS=false

# App Configuration
APP_VERSION=1.0.0
BUILD_NUMBER=1
